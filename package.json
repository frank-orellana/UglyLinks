{
  "name": "uglylinks",
  "version": "0.1.0",
  "description": "Uglylinks WebExtension, URL Uglifier",
  "displayName": "UglyLinks",
  "scripts": {
    "build": "tsc && webpack && npm run post_compile",
    "build-zip": "ts-node build.ts",
    "buildw": "concurrently --kill-others \"npm run build_wp_w\" \"npm run build_tsc_w\"",
    "build_wp_w": "webpack --watch",
    "build_tsc_w": "tsc-watch --onSuccess \"npm run post_compile\"",
    "post_compile": "\"node correct-module.js\"",
    "build-tsc-w-simple": "tsc -w",
    "test": "mocha --opts tests/mocha.opts",
    "webpack": "webpack",
    "ts-node": "ts-node "
  },
  "author": "Franklin Rolando Orellana Pineda",
  "license": "GPL-3.0-or-later",
  "devDependencies": {
    "@types/chai": "^4.1.6",
    "@types/jquery": "^3.3.21",
    "@types/mocha": "^2.2.48",
    "@types/node": "^10.12.0",
    "chai": "^4.2.0",
    "concurrently": "^4.0.1",
    "mocha": "^4.1.0",
    "ts-loader": "^5.2.2",
    "ts-node": "^7.0.1",
    "tsc-watch": "^1.0.30",
    "typescript": "^2.9.2",
    "vue": "^2.5.17",
    "web-ext-types": "^2.3.0",
    "webpack": "^4.21.0",
    "webpack-cli": "^3.1.2"
  },
  "dependencies": {},
  "repository": {
    "type": "git",
    "url": "https://github.com/frank-orellana/UglyLinks"
  },
  "bugs": {
    "url": "https://github.com/frank-orellana/UglyLinks/issues"
  },
  "keywords": [
    "WebExtensions"
  ]
}
